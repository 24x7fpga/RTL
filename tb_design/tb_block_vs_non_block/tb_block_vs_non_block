    .scope S_0x5b141f04ef20;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x5b141f0a1e50_0;
    %pushi/vec4 0, 0, 1;
    %cmp/e;
    %flag_get/vec4 6;
    %store/vec4 v0x5b141f0a1e50_0, 0, 1;
    %jmp T_3;
