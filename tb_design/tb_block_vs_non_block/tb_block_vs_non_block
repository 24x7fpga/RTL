T_1.0 ;
    %load/vec4 v0x5b141f050ba0_0;
    %load/vec4 v0x5b141f0a15a0_0;
    %or;
    %store/vec4 v0x5b141f0a1990_0, 0, 1;
    %load/vec4 v0x5b141f0a1990_0;
    %load/vec4 v0x5b141f0a1660_0;
    %xor;
    %store/vec4 v0x5b141f0a1b10_0, 0, 1;
    %load/vec4 v0x5b141f0a1b10_0;
    %store/vec4 v0x5b141f0a1c90_0, 0, 1;
T_1.1 ;
    %jmp T_1;
