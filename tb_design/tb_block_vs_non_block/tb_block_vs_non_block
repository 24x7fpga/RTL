    .scope S_0x5b141f08a490;
T_2 ;
    %wait E_0x5b141f08bb50;
    %load/vec4 v0x5b141f0a17c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b141f0a18d0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b141f0a1a50_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b141f0a1bd0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x5b141f050ba0_0;
    %load/vec4 v0x5b141f0a15a0_0;
    %or;
    %assign/vec4 v0x5b141f0a18d0_0, 0;
    %load/vec4 v0x5b141f0a18d0_0;
    %load/vec4 v0x5b141f0a1660_0;
    %xor;
    %assign/vec4 v0x5b141f0a1a50_0, 0;
    %load/vec4 v0x5b141f0a1a50_0;
    %assign/vec4 v0x5b141f0a1bd0_0, 0;
T_2.1 ;
    %jmp T_2;
