T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b141f050ba0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b141f0a15a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b141f0a1660_0, 0, 1;
    %end;
    .thread T_0, $init;
    .scope S_0x5b141f08a490;
T_1 ;
Ewait_0 .event/or E_0x5b141f08b890, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x5b141f0a17c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b141f0a1990_0, 0, 1;
    %pushi/vec4 0, 0, 1;
