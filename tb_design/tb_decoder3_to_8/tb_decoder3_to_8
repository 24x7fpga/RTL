#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55f34a27e5c0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55f34a2560f0 .scope module, "tb_decoder3_to_8" "tb_decoder3_to_8" 3 2;
 .timescale -9 -9;
P_0x55f34a27da30 .param/l "t" 1 3 3, +C4<00000000000000000000000000001010>;
P_0x55f34a27da70 .param/l "width" 1 3 4, +C4<00000000000000000000000000000011>;
v0x55f34a2a55c0_0 .var "in", 2 0;
v0x55f34a2a56b0_0 .net "out", 7 0, v0x55f34a2a5480_0;  1 drivers
S_0x55f34a256350 .scope module, "DUT" "decoder3_to_8" 3 15, 4 2 0, S_0x55f34a2560f0;
 .timescale -9 -9;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 3 "in";
P_0x55f34a256530 .param/l "width" 0 4 2, +C4<00000000000000000000000000000011>;
v0x55f34a27caa0_0 .net "in", 2 0, v0x55f34a2a55c0_0;  1 drivers
v0x55f34a2a5480_0 .var "out", 7 0;
E_0x55f34a2936d0 .event edge, v0x55f34a27caa0_0;
    .scope S_0x55f34a256350;
T_0 ;
Ewait_0 .event/or E_0x55f34a2936d0, E_0x0;
    %wait Ewait_0;
    %pushi/vec4 1, 0, 8;
    %ix/getv 4, v0x55f34a27caa0_0;
    %shiftl 4;
    %store/vec4 v0x55f34a2a5480_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55f34a2560f0;
T_1 ;
    %pushi/vec4 50, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 3 25 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000000111 {0 0 0};
    %pad/u 3;
    %store/vec4 v0x55f34a2a55c0_0, 0, 3;
    %delay 10, 0;
    %load/vec4 v0x55f34a2a56b0_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x55f34a2a55c0_0;
    %pow;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 2, 0, 32;
    %load/vec4 v0x55f34a2a55c0_0;
    %pow;
    %vpi_call/w 3 29 "$display", "Input = %0d Expected result = %0d, Result = %0d", v0x55f34a2a55c0_0, S<0,vec4,s32>, v0x55f34a2a56b0_0 {1 0 0};
    %jmp T_1.3;
T_1.2 ;
    %vpi_call/w 3 31 "$display", "Simulation FAIL ;( \012" {0 0 0};
    %vpi_call/w 3 32 "$finish" {0 0 0};
T_1.3 ;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 35 "$display", "Simulation PASS ;) \012" {0 0 0};
    %vpi_call/w 3 36 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55f34a2560f0;
T_2 ;
    %vpi_call/w 3 40 "$dumpfile", "tb_decoder3_to_8.vcd" {0 0 0};
    %vpi_call/w 3 41 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55f34a2560f0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
