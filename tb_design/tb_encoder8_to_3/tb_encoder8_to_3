:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x648a9017b190 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x648a9017b320 .scope module, "tb_encoder8_to_3" "tb_encoder8_to_3" 3 2;
 .timescale -9 -9;
P_0x648a901a02f0 .param/l "t" 1 3 3, +C4<00000000000000000000000000001010>;
P_0x648a901a0330 .param/l "width" 1 3 4, +C4<00000000000000000000000000001000>;
v0x648a901cd420_0 .var "in", 7 0;
v0x648a901cd510_0 .var "k", 2 0;
v0x648a901cd5d0_0 .net "out", 2 0, v0x648a901cd2e0_0;  1 drivers
S_0x648a901b7b50 .scope module, "DUT" "encoder8_to_3" 3 16, 4 2 0, S_0x648a9017b320;
 .timescale -9 -9;
    .port_info 0 /OUTPUT 3 "out";
    .port_info 1 /INPUT 8 "in";
P_0x648a901b7d30 .param/l "width" 0 4 2, +C4<00000000000000000000000000001000>;
v0x648a901cd1e0_0 .net "in", 7 0, v0x648a901cd420_0;  1 drivers
v0x648a901cd2e0_0 .var "out", 2 0;
E_0x648a901b8bb0 .event edge, v0x648a901cd1e0_0;
S_0x648a901b7dd0 .scope begin, "$ivl_for_loop0" "$ivl_for_loop0" 4 24, 4 24 0, S_0x648a901b7b50;
 .timescale -9 -9;
v0x648a901a5f50_0 .var/2s "i", 31 0;
    .scope S_0x648a901b7b50;
T_0 ;
Ewait_0 .event/or E_0x648a901b8bb0, E_0x0;
    %wait Ewait_0;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x648a901cd2e0_0, 0, 3;
    %fork t_1, S_0x648a901b7dd0;
    %jmp t_0;
    .scope S_0x648a901b7dd0;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x648a901a5f50_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x648a901a5f50_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x648a901cd1e0_0;
    %load/vec4 v0x648a901a5f50_0;
    %part/s 1;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x648a901a5f50_0;
    %pad/s 3;
    %store/vec4 v0x648a901cd2e0_0, 0, 3;
T_0.2 ;
    %load/vec4 v0x648a901a5f50_0;
    %addi 1, 0, 32;
    %cast2;
    %store/vec4 v0x648a901a5f50_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x648a901b7b50;
t_0 %join;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x648a9017b320;
T_1 ;
    %pushi/vec4 50, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 3 27 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000000111 {0 0 0};
    %pad/u 3;
    %store/vec4 v0x648a901cd510_0, 0, 3;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x648a901cd420_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x648a901cd510_0;
    %store/vec4 v0x648a901cd420_0, 4, 1;
    %delay 10, 0;
    %vpi_call/w 3 31 "$display", "Exp Result = %0d, Result = %0d", v0x648a901cd510_0, v0x648a901cd5d0_0 {0 0 0};
    %load/vec4 v0x648a901cd510_0;
    %load/vec4 v0x648a901cd5d0_0;
    %cmp/ne;
    %jmp/0xz  T_1.2, 4;
    %vpi_call/w 3 33 "$display", "simulation FAIL ;(\012" {0 0 0};
    %vpi_call/w 3 34 "$finish" {0 0 0};
T_1.2 ;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 37 "$display", "Simulation PASS ;)" {0 0 0};
    %vpi_call/w 3 38 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x648a9017b320;
T_2 ;
    %vpi_call/w 3 45 "$dumpfile", "tb_encoder8_to_3.vcd" {0 0 0};
    %vpi_call/w 3 46 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x648a9017b320 {0 0 0};
    %end;
    .thread T_2;
