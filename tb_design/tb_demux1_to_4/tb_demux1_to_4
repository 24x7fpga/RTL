S_0x5570baf48f20 .scope module, "DUT" "demux1_to_4" 2 11, 3 1 0, S_0x5570baf48d90;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "z";
    .port_info 1 /INPUT 1 "din";
    .port_info 2 /INPUT 2 "sel";
v0x5570baf4a7b0_0 .net "din", 0 0, v0x5570baf963d0_0;  1 drivers
v0x5570baf4aba0_0 .net "sel", 1 0, v0x5570baf964a0_0;  1 drivers
v0x5570baf96290_0 .var "z", 3 0;
E_0x5570baf84990 .event edge, v0x5570baf4aba0_0, v0x5570baf4a7b0_0;
    .scope S_0x5570baf48f20;
T_0 ;
    %wait E_0x5570baf84990;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5570baf96290_0, 0, 4;
    %load/vec4 v0x5570baf4aba0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x5570baf4a7b0_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf96290_0, 4, 1;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x5570baf4a7b0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf96290_0, 4, 1;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x5570baf4a7b0_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf96290_0, 4, 1;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x5570baf4a7b0_0;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf96290_0, 4, 1;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5570baf48d90;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5570baf963d0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5570baf964a0_0, 0, 2;
    %end;
    .thread T_1;
    .scope S_0x5570baf48d90;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5570baf963d0_0;
    %inv;
    %store/vec4 v0x5570baf963d0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5570baf48d90;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x5570baf964a0_0;
    %parti/s 1, 0, 2;
    %inv;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf964a0_0, 4, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5570baf48d90;
T_4 ;
    %delay 20, 0;
    %load/vec4 v0x5570baf964a0_0;
    %parti/s 1, 1, 2;
    %inv;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5570baf964a0_0, 4, 1;
