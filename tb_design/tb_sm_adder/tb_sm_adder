S_0x55e37a5efa00 .scope module, "tb_sm_adder" "tb_sm_adder" 2 1;
 .timescale 0 0;
P_0x55e37a5b3ea0 .param/l "n" 0 2 2, +C4<00000000000000000000000000000100>;
v0x55e37a6043b0_0 .var "opA", 3 0;
v0x55e37a6044a0_0 .var "opB", 3 0;
v0x55e37a604570_0 .net "res", 3 0, v0x55e37a604240_0;  1 drivers
v0x55e37a604670_0 .var "temp", 3 0;
S_0x55e37a5efbe0 .scope module, "DUT" "sm_adder" 2 13, 3 9 0, S_0x55e37a5efa00;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "res";
    .port_info 1 /INPUT 4 "opA";
    .port_info 2 /INPUT 4 "opB";
P_0x55e37a5efdc0 .param/l "n" 0 3 9, +C4<00000000000000000000000000000100>;
v0x55e37a5db690_0 .net "opA", 3 0, v0x55e37a6043b0_0;  1 drivers
v0x55e37a604160_0 .net "opB", 3 0, v0x55e37a6044a0_0;  1 drivers
v0x55e37a604240_0 .var "res", 3 0;
E_0x55e37a5ebd30 .event edge, v0x55e37a604160_0, v0x55e37a5db690_0;
    .scope S_0x55e37a5efbe0;
T_0 ;
    %wait E_0x55e37a5ebd30;
    %load/vec4 v0x55e37a5db690_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x55e37a604160_0;
    %parti/s 1, 3, 3;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x55e37a5db690_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e37a604240_0, 4, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e37a5db690_0;
    %load/vec4 v0x55e37a604160_0;
    %cmp/u;
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x55e37a604160_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e37a604240_0, 4, 1;
    %load/vec4 v0x55e37a604160_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x55e37a5db690_0;
    %parti/s 3, 0, 2;
    %sub;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e37a604240_0, 4, 3;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55e37a5db690_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e37a604240_0, 4, 1;
    %load/vec4 v0x55e37a5db690_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x55e37a604160_0;
    %parti/s 3, 0, 2;
    %sub;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55e37a604240_0, 4, 3;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55e37a5efa00;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "tb_sm_adder.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e37a5efa00 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55e37a5efa00;
T_2 ;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55e37a6043b0_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x55e37a6044a0_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55e37a5efa00;
T_3 ;
    %load/vec4 v0x55e37a6044a0_0;
    %parti/s 3, 0, 2;
    %pad/u 4;
    %load/vec4 v0x55e37a6043b0_0;
    %parti/s 3, 0, 2;
    %pad/u 4;
    %sub;
    %store/vec4 v0x55e37a604670_0, 0, 4;
    %end;
    .thread T_3;
